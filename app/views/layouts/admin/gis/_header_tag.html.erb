<meta http-equiv="content-type" content="text/html;charset=UTF-8" />
<meta http-equiv="content-style-type" content="text/css" />
<meta http-equiv="content-script-type" content="text/javascript" />
<link rel="shortcut icon" href="/images/favicon.ico"  />
<title><%= Page.title || @page_title %> - <%= Site.title %></title>

<%= javascript_include_tag "/javascripts/jquery.min.js" %>
<%= javascript_include_tag "/javascripts/jquery-ui.js" %>
<%= javascript_include_tag "/javascripts/rails.js" %>

<%=raw @js.map{|js| javascript_include_tag js}.join("\n") if @js %>
<%=raw @dynamic_js.map{|js| javascript_include_tag url_for({:controller => js[:controller], :action => js[:action]})}.join("\n") if @dynamic_js %>
<%= stylesheet_link_tag :application %>
<%= stylesheet_link_tag '/layout/gis/base.css' %>
<%= stylesheet_link_tag '/_common/themes/gis/css/print.css', :media=>'print' %>
<%=raw @css.map{|css| stylesheet_link_tag css}.join("\n") if @css %>
