<%= form_for :item, :url => {:action => :create}, :html => {:id => 'itemForm', :multipart => true} do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%= button_tag '戻る', :type => :button, :onclick => "document.location='#{url_for(:action => :index)}';" %>
  <%= f.submit 'この内容で登録する' %>
<% end %>

