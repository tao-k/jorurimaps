<%= form_for :item, :url => {:action => :new_confirm}, :html => {:id => 'itemForm'} do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form_csv', :locals => {:f => f} %>
  <%= button_tag '戻る', :type => :button, :onclick => "document.location='#{url_for(:action => :index)}';" %>
  <%= f.submit '入力内容を確認する' %>
<% end %>
