<table class="index">
  
  <tr>
    <th style="width: 60px; text-align: center;">詳細</th>
    <th style="width:; text-align: center;">バージョン</th>
    <th style="width: 120px; text-align: center;">作成日時</th>
  </tr>
  
  <% for item in @items %>
  <tr <%= cycle '', 'class="cycle"' %>>
    <td class="showLink"><%= link_to :show, url_for(:action => :show, :id => item.version) %></td>
    <td><%=h item.version%></td>
    <td><%=h safe{item.created_at.strftime('%Y-%m-%d %H:%M')} %></td>
  </tr>
  <% end %>

</table>

<%= paginate @items %>
