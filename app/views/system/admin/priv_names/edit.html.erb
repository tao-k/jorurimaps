<div class="piece AdminMessage">
<div class="pieceBody">

<%= form_for :item, :url => url_for({:action=>:update}), :html => {:method => :put, :multipart => true, :id => 'itemForm'} do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%= submit_for(f, {:caption => '保存'}) %>
<% end %>

</div>
</div>
