<div class="piece AdminMessage">


<div class="pieceBody">

<table class="index">
  <tbody>
    <tr>
      <th>詳細</th>
      <th>権限対象名</th>
      <th>権限コード</th>
      <th style="width: 40px; text-align: center;">表示順</th>
      <th style="width: 60px; text-align: center;">ステータス</th>
    </tr>
    <% for item in @items %>
    <tr class="<%= cycle '', 'cycle' %>">
      <td class="showLink"><%= link_to '詳細', "#{url_for({:action=>:show, :id=>item.id})}?#{@qs}" %></td>
      <td><%= item.display_name %></td>
      <td><%= item.priv_name %></td>
      <td style="text-align: center;"><%= item.sort_no %></td>
      <td style="text-align: center;"><%= item.state_label %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<br/>
<%= paginate @items %>
<br/>

</div>
</div>
